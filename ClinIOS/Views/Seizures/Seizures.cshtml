        var request = $http.get(baseUrl + "api/Seizures?secName=SZ_VitalsPop&Id=1").then(function (response) { if (response.data.Id != null) { $scope.VitalsPop = response.data; json_VitalsPop = response.data;   /* insertRecord('chestpain', json)*/ } });
