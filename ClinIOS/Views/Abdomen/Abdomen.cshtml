                                                                @*<td class="align-center">Alert</td>*@
                                                                <td class="align-center">Class1</td>
                                                                <td class="align-center">&gt;750ml</td>
                                                            </tr>
                                                            <tr class="odd">
                                                                <td class="align-center"><input type="button" class="btn-unselected" value="&gt;100" /></td>
                                                                <td class="align-center"><input type="button" class="btn-unselected" value="&gt;100" /></td>
                                                                <td class="align-center"><input type="button" class="btn-unselected" value="20 - 30" /></td>
                                                                <td class="align-center">@*Anxious*@</td>
                                                                <td class="align-center">Class2</td>
                                                                <td class="align-center">750-1500ml</td>
                                                            </tr>
                                                            <tr class="even">
                                                                <td class="align-center"><input type="button" class="btnselected" value="&gt;120" /></td>
                                                                <td class="align-center"><input type="button" class="btnselected" value="&lt;100" /></td>
                                                                <td class="align-center"><input type="button" class="btnselected" value="30 - 40" /></td>
                                                                <td class="align-center">@*Confused*@</td>
                                                                <td class="align-center">Class3</td>
                                                                <td class="align-center">1500-2000ml</td>
