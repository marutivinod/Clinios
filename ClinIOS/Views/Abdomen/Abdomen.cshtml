                                                    <label class="sec-label">Patent:</label>
                                                    <span class="sec-fields">
                                                        <span class="radiospan">
                                                            <input ng-repeat="item in ddValues.YesNo"
                                                                   type='button'
                                                                   class="btn-unselected"
                                                                   ng-class="{'btnselected': PAAir.A_PatentId == {{item.Id}}}"
                                                                   value='{{item.Name}}'
                                                                   ng-click="buttonSelect('A_PatentId',item.Id,'PAAir')"
                                                                   ng-model="PAAir.A_PatentId">
                                                        </span>
                                                    </span>
                                                </div>
                                                <div class="sec-row" ng-if="PAAir.A_PatentId == '29'">
