                                                        <span class="radiospan"><input type="checkbox" ng-model="PAAir.A_FrnBodyInd">Foreign Body</span>
                                                        <span class="radiospan"><input type="checkbox" ng-model="PAAir.A_FluidInd">Fluids</span>
                                                    </span>
                                                </div>
                                                <div class="sec-row">
                                                    <label class="sec-label">Protected:</label>
                                                    <span class="sec-fields">
                                                        <span class="radiospan">
                                                            <input ng-repeat="item in ddValues.YesNo"
                                                                   type='button'
                                                                   class="btn-unselected"
                                                                   ng-class="{'btnselected': PAAir.A_ProtectedInd == {{item.Id}}}"
                                                                   value='{{item.Name}}'
                                                                   ng-click="buttonSelect('A_ProtectedInd',item.Id,'PAAir')"
                                                                   ng-model="PAAir.A_ProtectedInd">
                                                        </span>
                                                    </span>
                                                    <span class="sec-fields">
                                                        <span class="radiospan">
                                                            <input ng-repeat="item in ddValues.Asmnt_Protected"
                                                                   type='button'
                                                                   class="btn-unselected"
                                                                   ng-class="{'btnselected': PAAir.A_ProtectedId == {{item.Id}}}"
                                                                   value='{{item.Name}}'
                                                                   ng-click="buttonSelect('A_ProtectedId',item.Id,'PAAir')"
                                                                   ng-model="PAAir.A_ProtectedId">
                                                        </span>
                                                    </span>
                                                </div><br />

                                                <div class="sec-row">
                                                    <label>C-SPLINE IMMOBLIZATION:</label>
