        $scope.calcEvmTotal = function (EVM, resl) {
            switch (EVM) {
                case 'E': $scope.EyeOpen = resl; break; //$("#E").val(resl)
                case 'V': $scope.VerbalResp = resl; break; //$("#V").val(resl)
                case 'M': $scope.MotorResp = resl; break; //$("#M").val(resl)
            }
            $scope.EVMTotal = $scope.EyeOpen + $scope.VerbalResp + $scope.MotorResp;
        };
