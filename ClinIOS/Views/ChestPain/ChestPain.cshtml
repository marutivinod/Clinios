    function nextBlock() {
        currentId++;
        if (currentId >= totalBlock) currentId = 0;
        $(".navigation-block").hide();
        $($(".navigation-block")[currentId]).show();
        navigationLinks();
        nextPrevDisplay();
    }

    function previousBlock() {
        currentId--;
        if (currentId < 0) currentId = totalBlock - 1;
        $(".navigation-block").hide();
        $($(".navigation-block")[currentId]).show();
        navigationLinks();
        nextPrevDisplay();
        console.log('pervious clicked');
        $('.last-section-info').remove();
    }

    function gotoBlock(blockId) {
        currentId = blockId;
        $(".navigation-block").hide();
        $($(".navigation-block")[currentId]).show();
        navigationLinks();
        nextPrevDisplay();
    }

    function navigationLinks() {
        $(".navigation-links").html("");
        $('.last-section-info').remove();
        for (var l = 0; l < currentId + 1; l++) {
            $(".navigation-links").append("<li><a href=# onclick=gotoBlock(" + l + ")>" + links[l] + "</a></li>")
        }
    }

    function nextPrevDisplay() {
        $("#next-block").show();
        $("#previous-block").show();

        if (currentId == 0) {
            $("#previous-block").hide();
        }
        if (currentId == totalBlock - 1) {
            $("#next-block").hide();
        }

        if (currentId === links.length -1) {
            getFinalRwsult();
        }
    }
    isEmpty = (obj) => {
        for (var key in obj) {
            if (obj.hasOwnProperty(key))
                return true;
        }
        return false;
    }
    
